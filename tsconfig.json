{
  "compilerOptions": {
    "target": "ES6",
    "lib": [ "ES2015" ],                                       
    "module": "commonjs",
    "rootDir": "src",
    "moduleResolution": "Node",
    "outDir": "dist",
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
}
